<odoo>


#Doctor type Tree view
	<record id="view_doctor_type_tree" model="ir.ui.view">

		<field name="name">doctortype.tree</field>
		<field name="model">hospital.typesofdoc</field>
		<field name="arch" type="xml">
			
			<tree>
				<field name="name"></field>

			</tree>
			
		</field>

	</record>
	
	#Dotor Types Form view

	<record id="view_doctype_form" model="ir.ui.view">

		<field name="name">doctype.form</field>
		<field name="model">hospital.typesofdoc</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="name"></field>
						
					</group>
				</sheet>
				

			</form>
		</field>

	</record>

</odoo>